<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="270.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.length_scene_controller">
   <children>
      <ComboBox fx:id="FomComboboxID" layoutX="21.0" layoutY="84.0" prefWidth="150.0" promptText="FROM">
         <effect>
            <DropShadow />
         </effect>
      </ComboBox>
      <ComboBox fx:id="ToComboBoxID" layoutX="21.0" layoutY="136.0" prefWidth="150.0" promptText="TO">
         <effect>
            <DropShadow />
         </effect>
      </ComboBox>
      <TextField fx:id="FromTextField" layoutX="186.0" layoutY="84.0" prefHeight="29.0" prefWidth="149.0" promptText="Enter">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font></TextField>
      <TextField fx:id="TOTextField" editable="false" layoutX="186.0" layoutY="136.0" />
      <Button fx:id="ConvertButton" layoutX="145.0" layoutY="189.0" mnemonicParsing="false" onAction="#onActionConvert" text="CONVERT">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Hyperlink fx:id="HYperLinkID" layoutX="120.0" layoutY="228.0" onAction="#SwitchToHomePage" prefHeight="28.0" prefWidth="127.0" text="Return To HomePage">
         <font>
            <Font name="Agency FB Bold" size="16.0" />
         </font>
      </Hyperlink>
      <Label layoutX="113.0" layoutY="23.0" prefHeight="33.0" prefWidth="141.0" text="LENGTH CONVERTOR">
         <font>
            <Font name="Agency FB Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
